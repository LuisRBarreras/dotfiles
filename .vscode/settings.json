{
	"activitusbar.views": [
		{
			"codicon": "files",
			"name": "explorer"
		},
		{
			"codicon": "search",
			"name": "search"
		},
	
		{
			"codicon": "debug-alt",
			"name": "debug"
		},
		{
			"codicon": "source-control",
			"name": "scm"
		},
		{
			"codicon": "three-bars",
			"name": "extension.bookmarks"
		},
		{
			"codicon": "database",
			"name": "extension.dockerView"
		},
		{
			"codicon": "extensions",
			"name": "extensions"
		}
	],
	"bookmarks.navigateThroughAllFiles": true,
	"bookmarks.sideBar.expanded": true,
	"breadcrumbs.enabled": false,
	"cSpell.userWords": [
		"Barreras",
		"summitwealth",
		"typeorm"
	],
	"diffEditor.ignoreTrimWhitespace": true,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.folding": true,
	"editor.fontFamily": "Menlo, Monaco, 'Courier New', Monaco",
	"editor.glyphMargin": true,
	"editor.lineNumbers": "on",
	"editor.minimap.enabled": false,
	"editor.rulers": [
		120,
		80
	],
	"editor.tabSize": 2,
	"editor.trimAutoWhitespace": false,
	"files.exclude": {
		"node_modules": true
	},
	"files.trimTrailingWhitespace": false,
	"git.ignoreMissingGitWarning": true,
	"inline-parameters.margin": "0.10em",
	"inline-parameters.padding": "0.10em",
	"standard.enable": true,
	"standard.enableGlobally": true,
	"standard.treatErrorsAsWarnings": true,
	"standard.usePackageJson": false,
	"terminal.external.osxExec": "iTerminal.app",
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.fontFamily": "monospace",
	"typescript.format.enable": false,
	"vim.easymotion": true,
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false
	},
	"vim.hlsearch": true,
	"vim.incsearch": true,
	"vim.insertModeKeyBindings": [
		{
			"after": [
				"<Esc>"
			],
			"before": [
				"j",
				"j"
			]
		}
	],
	"vim.leader": "<space>",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"after": [
				"d",
				"d"
			],
			"before": [
				"<leader>",
				"d"
			]
		},
		{
			"before": [
				"<C-n>"
			],
			"commands": [
				":nohl"
			]
		},
		{
			"before": [
				"<leader>",
				"b"
			],
			"commands": [
				"bookmarks.listFromAllFiles"
			]
		},
		{
			"before": [
				"<leader>",
				"m"
			],
			"commands": [
				"bookmarks.toggle"
			]
		}
	],
	"vim.sneak": true,
	"vim.useCtrlKeys": true,
	"vim.useSystemClipboard": true,
	"vim.statusBarColorControl": true,
	"vim.statusBarColors.normal": [
		"#8FBCBB",
		"#434C5E"
	],
	"vim.statusBarColors.insert": "#BF616A",
	"vim.statusBarColors.visual": "#B48EAD",
	"vim.statusBarColors.visualline": "#B48EAD",
	"vim.statusBarColors.visualblock": "#A3BE8C",
	"vim.statusBarColors.replace": "#D08770",
	"vim.statusBarColors.commandlineinprogress": "#007ACC",
	"vim.statusBarColors.searchinprogressmode": "#007ACC",
	"vim.statusBarColors.easymotionmode": "#007ACC",
	"vim.statusBarColors.easymotioninputmode": "#007ACC",
	"vim.statusBarColors.surroundinputmode": "#007ACC",
	"vim.surround": true,
	"vim.visualModeKeyBindingsNonRecursive": [],
	"window.zoomLevel": 1,
	"workbench.activityBar.visible": false,
	"workbench.colorCustomizations": {
		"bookmarks.lineBorder": "#00e6e6",
		"editorRuler.foreground": "#ff4081",
		"statusBar.background": "#B48EAD",
		"statusBar.noFolderBackground": "#B48EAD",
		"statusBar.debuggingBackground": "#B48EAD",
		"statusBar.foreground": "#434C5E",
		"statusBar.debuggingForeground": "#434C5E"
	},
	"workbench.colorTheme": "Default Dark+"
	"emeraldwalk.runonsave": {
    	"commands": [
      		{
       			"match": "\\.js$",
        		"cmd": "standard --fix ${file}"
      		},
    	]
  }
}
